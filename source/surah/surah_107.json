    "name": "al-Ma'un",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "أَرَأَيْتَ الَّذِي يُكَذِّبُ بِالدِّينِ",
        "verse_2": "فَذَٰلِكَ الَّذِي يَدُعُّ الْيَتِيمَ",
        "verse_3": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ الْمِسْكِينِ",
        "verse_4": "فَوَيْلٌ لِلْمُصَلِّينَ",
        "verse_5": "الَّذِينَ هُمْ عَنْ صَلَاتِهِمْ سَاهُونَ",
        "verse_6": "الَّذِينَ هُمْ يُرَاءُونَ",
        "verse_7": "وَيَمْنَعُونَ الْمَاعُونَ"
    },

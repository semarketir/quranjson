{
    "index": "077",
    "name": "al-Mursalat",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "وَالْمُرْسَلَاتِ عُرْفًا",
        "verse_2": "فَالْعَاصِفَاتِ عَصْفًا",
        "verse_3": "وَالنَّاشِرَاتِ نَشْرًا",
        "verse_4": "فَالْفَارِقَاتِ فَرْقًا",
        "verse_5": "فَالْمُلْقِيَاتِ ذِكْرًا",
        "verse_6": "عُذْرًا أَوْ نُذْرًا",
        "verse_7": "إِنَّمَا تُوعَدُونَ لَوَاقِعٌ",
        "verse_8": "فَإِذَا النُّجُومُ طُمِسَتْ",
        "verse_9": "وَإِذَا السَّمَاءُ فُرِجَتْ",
        "verse_10": "وَإِذَا الْجِبَالُ نُسِفَتْ",
        "verse_11": "وَإِذَا الرُّسُلُ أُقِّتَتْ",
        "verse_12": "لِأَيِّ يَوْمٍ أُجِّلَتْ",
        "verse_13": "لِيَوْمِ الْفَصْلِ",
        "verse_14": "وَمَا أَدْرَاكَ مَا يَوْمُ الْفَصْلِ",
        "verse_15": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
        "verse_16": "أَلَمْ نُهْلِكِ الْأَوَّلِينَ",
        "verse_17": "ثُمَّ نُتْبِعُهُمُ الْآخِرِينَ",
        "verse_18": "كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَ",
        "verse_19": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
        "verse_20": "أَلَمْ نَخْلُقْكُمْ مِنْ مَاءٍ مَهِينٍ",
        "verse_21": "فَجَعَلْنَاهُ فِي قَرَارٍ مَكِينٍ",
        "verse_22": "إِلَىٰ قَدَرٍ مَعْلُومٍ",
        "verse_23": "فَقَدَرْنَا فَنِعْمَ الْقَادِرُونَ",
        "verse_24": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
        "verse_25": "أَلَمْ نَجْعَلِ الْأَرْضَ كِفَاتًا",
        "verse_26": "أَحْيَاءً وَأَمْوَاتًا",
        "verse_27": "وَجَعَلْنَا فِيهَا رَوَاسِيَ شَامِخَاتٍ وَأَسْقَيْنَاكُمْ مَاءً فُرَاتًا",
        "verse_28": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
        "verse_29": "انْطَلِقُوا إِلَىٰ مَا كُنْتُمْ بِهِ تُكَذِّبُونَ",
        "verse_30": "انْطَلِقُوا إِلَىٰ ظِلٍّ ذِي ثَلَاثِ شُعَبٍ",
        "verse_31": "لَا ظَلِيلٍ وَلَا يُغْنِي مِنَ اللَّهَبِ",
        "verse_32": "إِنَّهَا تَرْمِي بِشَرَرٍ كَالْقَصْرِ",
        "verse_33": "كَأَنَّهُ جِمَالَتٌ صُفْرٌ",
        "verse_34": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
        "verse_35": "هَٰذَا يَوْمُ لَا يَنْطِقُونَ",
        "verse_36": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ",
        "verse_37": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
        "verse_38": "هَٰذَا يَوْمُ الْفَصْلِ ۖ جَمَعْنَاكُمْ وَالْأَوَّلِينَ",
        "verse_39": "فَإِنْ كَانَ لَكُمْ كَيْدٌ فَكِيدُونِ",
        "verse_40": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
        "verse_41": "إِنَّ الْمُتَّقِينَ فِي ظِلَالٍ وَعُيُونٍ",
        "verse_42": "وَفَوَاكِهَ مِمَّا يَشْتَهُونَ",
        "verse_43": "كُلُوا وَاشْرَبُوا هَنِيئًا بِمَا كُنْتُمْ تَعْمَلُونَ",
        "verse_44": "إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ",
        "verse_45": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
        "verse_46": "كُلُوا وَتَمَتَّعُوا قَلِيلًا إِنَّكُمْ مُجْرِمُونَ",
        "verse_47": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
        "verse_48": "وَإِذَا قِيلَ لَهُمُ ارْكَعُوا لَا يَرْكَعُونَ",
        "verse_49": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
        "verse_50": "فَبِأَيِّ حَدِيثٍ بَعْدَهُ يُؤْمِنُونَ"
    },

{
    "index": "090",
    "name": "al-Balad",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "لَآ أُقْسِمُ بِهَٰذَا ٱلْبَلَدِ",
        "verse_2": "وَأَنتَ حِلٌّۢ بِهَٰذَا ٱلْبَلَدِ",
        "verse_3": "وَوَالِدٍۢ وَمَا وَلَدَ",
        "verse_4": "لَقَدْ خَلَقْنَا ٱلْإِنسَٰنَ فِى كَبَدٍ",
        "verse_5": "أَيَحْسَبُ أَن لَّن يَقْدِرَ عَلَيْهِ أَحَدٌۭ",
        "verse_6": "يَقُولُ أَهْلَكْتُ مَالًۭا لُّبَدًا",
        "verse_7": "أَيَحْسَبُ أَن لَّمْ يَرَهُۥٓ أَحَدٌ",
        "verse_8": "أَلَمْ نَجْعَل لَّهُۥ عَيْنَيْنِ",
        "verse_9": "وَلِسَانًۭا وَشَفَتَيْنِ",
        "verse_10": "وَهَدَيْنَٰهُ ٱلنَّجْدَيْنِ",
        "verse_11": "فَلَا ٱقْتَحَمَ ٱلْعَقَبَةَ",
        "verse_12": "وَمَآ أَدْرَىٰكَ مَا ٱلْعَقَبَةُ",
        "verse_13": "فَكُّ رَقَبَةٍ",
        "verse_14": "أَوْ إِطْعَٰمٌۭ فِى يَوْمٍۢ ذِى مَسْغَبَةٍۢ",
        "verse_15": "يَتِيمًۭا ذَا مَقْرَبَةٍ",
        "verse_16": "أَوْ مِسْكِينًۭا ذَا مَتْرَبَةٍۢ",
        "verse_17": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُوا۟ وَتَوَاصَوْا۟ بِٱلصَّبْرِ وَتَوَاصَوْا۟ بِٱلْمَرْحَمَةِ",
        "verse_18": "أُو۟لَٰٓئِكَ أَصْحَٰبُ ٱلْمَيْمَنَةِ",
        "verse_19": "وَٱلَّذِينَ كَفَرُوا۟ بِـَٔايَٰتِنَا هُمْ أَصْحَٰبُ ٱلْمَشْـَٔمَةِ",
        "verse_20": "عَلَيْهِمْ نَارٌۭ مُّؤْصَدَةٌۢ"
    },

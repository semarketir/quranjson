{
    "index": "082",
    "name": "al-Infithar",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "إِذَا ٱلسَّمَآءُ ٱنفَطَرَتْ",
        "verse_2": "وَإِذَا ٱلْكَوَاكِبُ ٱنتَثَرَتْ",
        "verse_3": "وَإِذَا ٱلْبِحَارُ فُجِّرَتْ",
        "verse_4": "وَإِذَا ٱلْقُبُورُ بُعْثِرَتْ",
        "verse_5": "عَلِمَتْ نَفْسٌۭ مَّا قَدَّمَتْ وَأَخَّرَتْ",
        "verse_6": "يَٰٓأَيُّهَا ٱلْإِنسَٰنُ مَا غَرَّكَ بِرَبِّكَ ٱلْكَرِيمِ",
        "verse_7": "ٱلَّذِى خَلَقَكَ فَسَوَّىٰكَ فَعَدَلَكَ",
        "verse_8": "فِىٓ أَىِّ صُورَةٍۢ مَّا شَآءَ رَكَّبَكَ",
        "verse_9": "كَلَّا بَلْ تُكَذِّبُونَ بِٱلدِّينِ",
        "verse_10": "وَإِنَّ عَلَيْكُمْ لَحَٰفِظِينَ",
        "verse_11": "كِرَامًۭا كَٰتِبِينَ",
        "verse_12": "يَعْلَمُونَ مَا تَفْعَلُونَ",
        "verse_13": "إِنَّ ٱلْأَبْرَارَ لَفِى نَعِيمٍۢ",
        "verse_14": "وَإِنَّ ٱلْفُجَّارَ لَفِى جَحِيمٍۢ",
        "verse_15": "يَصْلَوْنَهَا يَوْمَ ٱلدِّينِ",
        "verse_16": "وَمَا هُمْ عَنْهَا بِغَآئِبِينَ",
        "verse_17": "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلدِّينِ",
        "verse_18": "ثُمَّ مَآ أَدْرَىٰكَ مَا يَوْمُ ٱلدِّينِ",
        "verse_19": "يَوْمَ لَا تَمْلِكُ نَفْسٌۭ لِّنَفْسٍۢ شَيْـًۭٔا ۖ وَٱلْأَمْرُ يَوْمَئِذٍۢ لِّلَّهِ"
    },
    "count": 19,

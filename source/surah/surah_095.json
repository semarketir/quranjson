    "name": "at-Tin",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "بِّسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلتِّينِ وَٱلزَّيْتُونِ",
        "verse_2": "وَطُورِ سِينِينَ",
        "verse_3": "وَهَٰذَا ٱلْبَلَدِ ٱلْأَمِينِ",
        "verse_4": "لَقَدْ خَلَقْنَا ٱلْإِنسَٰنَ فِىٓ أَحْسَنِ تَقْوِيمٍۢ",
        "verse_5": "ثُمَّ رَدَدْنَٰهُ أَسْفَلَ سَٰفِلِينَ",
        "verse_6": "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ فَلَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍۢ",
        "verse_7": "فَمَا يُكَذِّبُكَ بَعْدُ بِٱلدِّينِ",
        "verse_8": "أَلَيْسَ ٱللَّهُ بِأَحْكَمِ ٱلْحَٰكِمِينَ"
    },

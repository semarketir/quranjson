{
    "index": "091",
    "name": "Ash-Shams",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "وَٱلشَّمْسِ وَضُحَىٰهَا",
        "verse_2": "وَٱلْقَمَرِ إِذَا تَلَىٰهَا",
        "verse_3": "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا",
        "verse_4": "وَٱلَّيْلِ إِذَا يَغْشَىٰهَا",
        "verse_5": "وَٱلسَّمَآءِ وَمَا بَنَىٰهَا",
        "verse_6": "وَٱلْأَرْضِ وَمَا طَحَىٰهَا",
        "verse_7": "وَنَفْسٍۢ وَمَا سَوَّىٰهَا",
        "verse_8": "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَىٰهَا",
        "verse_9": "قَدْ أَفْلَحَ مَن زَكَّىٰهَا",
        "verse_10": "وَقَدْ خَابَ مَن دَسَّىٰهَا",
        "verse_11": "كَذَّبَتْ ثَمُودُ بِطَغْوَىٰهَآ",
        "verse_12": "إِذِ ٱنۢبَعَثَ أَشْقَىٰهَا",
        "verse_13": "فَقَالَ لَهُمْ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقْيَٰهَا",
        "verse_14": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُم بِذَنۢبِهِمْ فَسَوَّىٰهَا",
        "verse_15": "وَلَا يَخَافُ عُقْبَٰهَا"
    },
    "count": 15,
    "juz": [
        {
            "index": "30",
            "verse": {
                "start": "verse_1",
                "end": "verse_15"
            }
        }

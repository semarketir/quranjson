{
    "index": "088",
    "name": "al-Ghasyiyah",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "هَلْ أَتَاكَ حَدِيثُ الْغَاشِيَةِ",
        "verse_2": "وُجُوهٌ يَوْمَئِذٍ خَاشِعَةٌ",
        "verse_3": "عَامِلَةٌ نَاصِبَةٌ",
        "verse_4": "تَصْلَىٰ نَارًا حَامِيَةً",
        "verse_5": "تُسْقَىٰ مِنْ عَيْنٍ آنِيَةٍ",
        "verse_6": "لَيْسَ لَهُمْ طَعَامٌ إِلَّا مِنْ ضَرِيعٍ",
        "verse_7": "لَا يُسْمِنُ وَلَا يُغْنِي مِنْ جُوعٍ",
        "verse_8": "وُجُوهٌ يَوْمَئِذٍ نَاعِمَةٌ",
        "verse_9": "لِسَعْيِهَا رَاضِيَةٌ",
        "verse_10": "فِي جَنَّةٍ عَالِيَةٍ",
        "verse_11": "لَا تَسْمَعُ فِيهَا لَاغِيَةً",
        "verse_12": "فِيهَا عَيْنٌ جَارِيَةٌ",
        "verse_13": "فِيهَا سُرُرٌ مَرْفُوعَةٌ",
        "verse_14": "وَأَكْوَابٌ مَوْضُوعَةٌ",
        "verse_15": "وَنَمَارِقُ مَصْفُوفَةٌ",
        "verse_16": "وَزَرَابِيُّ مَبْثُوثَةٌ",
        "verse_17": "أَفَلَا يَنْظُرُونَ إِلَى الْإِبِلِ كَيْفَ خُلِقَتْ",
        "verse_18": "وَإِلَى السَّمَاءِ كَيْفَ رُفِعَتْ",
        "verse_19": "وَإِلَى الْجِبَالِ كَيْفَ نُصِبَتْ",
        "verse_20": "وَإِلَى الْأَرْضِ كَيْفَ سُطِحَتْ",
        "verse_21": "فَذَكِّرْ إِنَّمَا أَنْتَ مُذَكِّرٌ",
        "verse_22": "لَسْتَ عَلَيْهِمْ بِمُصَيْطِرٍ",
        "verse_23": "إِلَّا مَنْ تَوَلَّىٰ وَكَفَرَ",
        "verse_24": "فَيُعَذِّبُهُ اللَّهُ الْعَذَابَ الْأَكْبَرَ",
        "verse_25": "إِنَّ إِلَيْنَا إِيَابَهُمْ",
        "verse_26": "ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُمْ"
    },
    "count": 26,
    "juz": [

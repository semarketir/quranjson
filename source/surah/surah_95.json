{
    "index": "095",
    "name": "at-Tin",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "وَالتِّينِ وَالزَّيْتُونِ",
        "verse_2": "وَطُورِ سِينِينَ",
        "verse_3": "وَهَٰذَا الْبَلَدِ الْأَمِينِ",
        "verse_4": "لَقَدْ خَلَقْنَا الْإِنْسَانَ فِي أَحْسَنِ تَقْوِيمٍ",
        "verse_5": "ثُمَّ رَدَدْنَاهُ أَسْفَلَ سَافِلِينَ",
        "verse_6": "إِلَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ فَلَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍ",
        "verse_7": "فَمَا يُكَذِّبُكَ بَعْدُ بِالدِّينِ",
        "verse_8": "أَلَيْسَ اللَّهُ بِأَحْكَمِ الْحَاكِمِينَ"
    },
    "count": 8,

{
    "index": "082",
    "name": "al-Infithar",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "إِذَا السَّمَاءُ انْفَطَرَتْ",
        "verse_2": "وَإِذَا الْكَوَاكِبُ انْتَثَرَتْ",
        "verse_3": "وَإِذَا الْبِحَارُ فُجِّرَتْ",
        "verse_4": "وَإِذَا الْقُبُورُ بُعْثِرَتْ",
        "verse_5": "عَلِمَتْ نَفْسٌ مَا قَدَّمَتْ وَأَخَّرَتْ",
        "verse_6": "يَا أَيُّهَا الْإِنْسَانُ مَا غَرَّكَ بِرَبِّكَ الْكَرِيمِ",
        "verse_7": "الَّذِي خَلَقَكَ فَسَوَّاكَ فَعَدَلَكَ",
        "verse_8": "فِي أَيِّ صُورَةٍ مَا شَاءَ رَكَّبَكَ",
        "verse_9": "كَلَّا بَلْ تُكَذِّبُونَ بِالدِّينِ",
        "verse_10": "وَإِنَّ عَلَيْكُمْ لَحَافِظِينَ",
        "verse_11": "كِرَامًا كَاتِبِينَ",
        "verse_12": "يَعْلَمُونَ مَا تَفْعَلُونَ",
        "verse_13": "إِنَّ الْأَبْرَارَ لَفِي نَعِيمٍ",
        "verse_14": "وَإِنَّ الْفُجَّارَ لَفِي جَحِيمٍ",
        "verse_15": "يَصْلَوْنَهَا يَوْمَ الدِّينِ",
        "verse_16": "وَمَا هُمْ عَنْهَا بِغَائِبِينَ",
        "verse_17": "وَمَا أَدْرَاكَ مَا يَوْمُ الدِّينِ",
        "verse_18": "ثُمَّ مَا أَدْرَاكَ مَا يَوْمُ الدِّينِ",
        "verse_19": "يَوْمَ لَا تَمْلِكُ نَفْسٌ لِنَفْسٍ شَيْئًا ۖ وَالْأَمْرُ يَوْمَئِذٍ لِلَّهِ"
    },
    "count": 19,
    "juz": [

    "name": "al-Qari'ah",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "الْقَارِعَةُ",
        "verse_2": "مَا الْقَارِعَةُ",
        "verse_3": "وَمَا أَدْرَاكَ مَا الْقَارِعَةُ",
        "verse_4": "يَوْمَ يَكُونُ النَّاسُ كَالْفَرَاشِ الْمَبْثُوثِ",
        "verse_5": "وَتَكُونُ الْجِبَالُ كَالْعِهْنِ الْمَنْفُوشِ",
        "verse_6": "فَأَمَّا مَنْ ثَقُلَتْ مَوَازِينُهُ",
        "verse_7": "فَهُوَ فِي عِيشَةٍ رَاضِيَةٍ",
        "verse_8": "وَأَمَّا مَنْ خَفَّتْ مَوَازِينُهُ",
        "verse_9": "فَأُمُّهُ هَاوِيَةٌ",
        "verse_10": "وَمَا أَدْرَاكَ مَا هِيَهْ",
        "verse_11": "نَارٌ حَامِيَةٌ"
    },

{
    "index": "094",
    "name": "Ash-Sharh",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "أَلَمْ نَشْرَحْ لَكَ صَدْرَكَ",
        "verse_2": "وَوَضَعْنَا عَنْكَ وِزْرَكَ",
        "verse_3": "الَّذِي أَنْقَضَ ظَهْرَكَ",
        "verse_4": "وَرَفَعْنَا لَكَ ذِكْرَكَ",
        "verse_5": "فَإِنَّ مَعَ الْعُسْرِ يُسْرًا",
        "verse_6": "إِنَّ مَعَ الْعُسْرِ يُسْرًا",
        "verse_7": "فَإِذَا فَرَغْتَ فَانْصَبْ",
        "verse_8": "وَإِلَىٰ رَبِّكَ فَارْغَبْ"
    },

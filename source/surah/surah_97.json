    "name": "al-Qadr",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "إِنَّا أَنْزَلْنَاهُ فِي لَيْلَةِ الْقَدْرِ",
        "verse_2": "وَمَا أَدْرَاكَ مَا لَيْلَةُ الْقَدْرِ",
        "verse_3": "لَيْلَةُ الْقَدْرِ خَيْرٌ مِنْ أَلْفِ شَهْرٍ",
        "verse_4": "تَنَزَّلُ الْمَلَائِكَةُ وَالرُّوحُ فِيهَا بِإِذْنِ رَبِّهِمْ مِنْ كُلِّ أَمْرٍ",
        "verse_5": "سَلَامٌ هِيَ حَتَّىٰ مَطْلَعِ الْفَجْرِ"
    },

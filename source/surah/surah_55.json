    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "الرَّحْمَٰنُ",
        "verse_2": "عَلَّمَ الْقُرْآنَ",
        "verse_3": "خَلَقَ الْإِنْسَانَ",
        "verse_4": "عَلَّمَهُ الْبَيَانَ",
        "verse_5": "الشَّمْسُ وَالْقَمَرُ بِحُسْبَانٍ",
        "verse_6": "وَالنَّجْمُ وَالشَّجَرُ يَسْجُدَانِ",
        "verse_7": "وَالسَّمَاءَ رَفَعَهَا وَوَضَعَ الْمِيزَانَ",
        "verse_8": "أَلَّا تَطْغَوْا فِي الْمِيزَانِ",
        "verse_9": "وَأَقِيمُوا الْوَزْنَ بِالْقِسْطِ وَلَا تُخْسِرُوا الْمِيزَانَ",
        "verse_10": "وَالْأَرْضَ وَضَعَهَا لِلْأَنَامِ",
        "verse_11": "فِيهَا فَاكِهَةٌ وَالنَّخْلُ ذَاتُ الْأَكْمَامِ",
        "verse_12": "وَالْحَبُّ ذُو الْعَصْفِ وَالرَّيْحَانُ",
        "verse_13": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
        "verse_14": "خَلَقَ الْإِنْسَانَ مِنْ صَلْصَالٍ كَالْفَخَّارِ",
        "verse_15": "وَخَلَقَ الْجَانَّ مِنْ مَارِجٍ مِنْ نَارٍ",
        "verse_16": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
        "verse_17": "رَبُّ الْمَشْرِقَيْنِ وَرَبُّ الْمَغْرِبَيْنِ",
        "verse_18": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
        "verse_19": "مَرَجَ الْبَحْرَيْنِ يَلْتَقِيَانِ",
        "verse_20": "بَيْنَهُمَا بَرْزَخٌ لَا يَبْغِيَانِ",
        "verse_21": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
        "verse_22": "يَخْرُجُ مِنْهُمَا اللُّؤْلُؤُ وَالْمَرْجَانُ",
        "verse_23": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
        "verse_24": "وَلَهُ الْجَوَارِ الْمُنْشَآتُ فِي الْبَحْرِ كَالْأَعْلَامِ",
        "verse_25": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
        "verse_26": "كُلُّ مَنْ عَلَيْهَا فَانٍ",
        "verse_27": "وَيَبْقَىٰ وَجْهُ رَبِّكَ ذُو الْجَلَالِ وَالْإِكْرَامِ",
        "verse_28": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",
        "verse_29": "يَسْأَلُهُ مَنْ فِي السَّمَاوَاتِ وَالْأَرْضِ ۚ كُلَّ يَوْمٍ هُوَ فِي شَأْنٍ",
        "verse_30": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ",

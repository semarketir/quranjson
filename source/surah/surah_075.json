{
    "index": "075",
    "name": "al-Qiyamah",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "لَآ أُقْسِمُ بِيَوْمِ ٱلْقِيَٰمَةِ",
        "verse_2": "وَلَآ أُقْسِمُ بِٱلنَّفْسِ ٱللَّوَّامَةِ",
        "verse_3": "أَيَحْسَبُ ٱلْإِنسَٰنُ أَلَّن نَّجْمَعَ عِظَامَهُۥ",
        "verse_4": "بَلَىٰ قَٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّىَ بَنَانَهُۥ",
        "verse_5": "بَلْ يُرِيدُ ٱلْإِنسَٰنُ لِيَفْجُرَ أَمَامَهُۥ",
        "verse_6": "يَسْـَٔلُ أَيَّانَ يَوْمُ ٱلْقِيَٰمَةِ",
        "verse_7": "فَإِذَا بَرِقَ ٱلْبَصَرُ",
        "verse_8": "وَخَسَفَ ٱلْقَمَرُ",
        "verse_9": "وَجُمِعَ ٱلشَّمْسُ وَٱلْقَمَرُ",
        "verse_10": "يَقُولُ ٱلْإِنسَٰنُ يَوْمَئِذٍ أَيْنَ ٱلْمَفَرُّ",
        "verse_11": "كَلَّا لَا وَزَرَ",
        "verse_12": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمُسْتَقَرُّ",
        "verse_13": "يُنَبَّؤُا۟ ٱلْإِنسَٰنُ يَوْمَئِذٍۭ بِمَا قَدَّمَ وَأَخَّرَ",
        "verse_14": "بَلِ ٱلْإِنسَٰنُ عَلَىٰ نَفْسِهِۦ بَصِيرَةٌۭ",
        "verse_15": "وَلَوْ أَلْقَىٰ مَعَاذِيرَهُۥ",
        "verse_16": "لَا تُحَرِّكْ بِهِۦ لِسَانَكَ لِتَعْجَلَ بِهِۦٓ",
        "verse_17": "إِنَّ عَلَيْنَا جَمْعَهُۥ وَقُرْءَانَهُۥ",
        "verse_18": "فَإِذَا قَرَأْنَٰهُ فَٱتَّبِعْ قُرْءَانَهُۥ",
        "verse_19": "ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُۥ",
        "verse_20": "كَلَّا بَلْ تُحِبُّونَ ٱلْعَاجِلَةَ",
        "verse_21": "وَتَذَرُونَ ٱلْءَاخِرَةَ",
        "verse_22": "وُجُوهٌۭ يَوْمَئِذٍۢ نَّاضِرَةٌ",
        "verse_23": "إِلَىٰ رَبِّهَا نَاظِرَةٌۭ",
        "verse_24": "وَوُجُوهٌۭ يَوْمَئِذٍۭ بَاسِرَةٌۭ",
        "verse_25": "تَظُنُّ أَن يُفْعَلَ بِهَا فَاقِرَةٌۭ",
        "verse_26": "كَلَّآ إِذَا بَلَغَتِ ٱلتَّرَاقِىَ",
        "verse_27": "وَقِيلَ مَنْ ۜ رَاقٍۢ",
        "verse_28": "وَظَنَّ أَنَّهُ ٱلْفِرَاقُ",
        "verse_29": "وَٱلْتَفَّتِ ٱلسَّاقُ بِٱلسَّاقِ",
        "verse_30": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمَسَاقُ",
        "verse_31": "فَلَا صَدَّقَ وَلَا صَلَّىٰ",
        "verse_32": "وَلَٰكِن كَذَّبَ وَتَوَلَّىٰ",
        "verse_33": "ثُمَّ ذَهَبَ إِلَىٰٓ أَهْلِهِۦ يَتَمَطَّىٰٓ",
        "verse_34": "أَوْلَىٰ لَكَ فَأَوْلَىٰ",
        "verse_35": "ثُمَّ أَوْلَىٰ لَكَ فَأَوْلَىٰٓ",
        "verse_36": "أَيَحْسَبُ ٱلْإِنسَٰنُ أَن يُتْرَكَ سُدًى",
        "verse_37": "أَلَمْ يَكُ نُطْفَةًۭ مِّن مَّنِىٍّۢ يُمْنَىٰ",
        "verse_38": "ثُمَّ كَانَ عَلَقَةًۭ فَخَلَقَ فَسَوَّىٰ",
        "verse_39": "فَجَعَلَ مِنْهُ ٱلزَّوْجَيْنِ ٱلذَّكَرَ وَٱلْأُنثَىٰٓ",
        "verse_40": "أَلَيْسَ ذَٰلِكَ بِقَٰدِرٍ عَلَىٰٓ أَن يُحْۦِىَ ٱلْمَوْتَىٰ"
    },

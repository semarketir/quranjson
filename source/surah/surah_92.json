{
    "index": "092",
    "name": " al-Lail ",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "وَاللَّيْلِ إِذَا يَغْشَىٰ",
        "verse_2": "وَالنَّهَارِ إِذَا تَجَلَّىٰ",
        "verse_3": "وَمَا خَلَقَ الذَّكَرَ وَالْأُنْثَىٰ",
        "verse_4": "إِنَّ سَعْيَكُمْ لَشَتَّىٰ",
        "verse_5": "فَأَمَّا مَنْ أَعْطَىٰ وَاتَّقَىٰ",
        "verse_6": "وَصَدَّقَ بِالْحُسْنَىٰ",
        "verse_7": "فَسَنُيَسِّرُهُ لِلْيُسْرَىٰ",
        "verse_8": "وَأَمَّا مَنْ بَخِلَ وَاسْتَغْنَىٰ",
        "verse_9": "وَكَذَّبَ بِالْحُسْنَىٰ",
        "verse_10": "فَسَنُيَسِّرُهُ لِلْعُسْرَىٰ",
        "verse_11": "وَمَا يُغْنِي عَنْهُ مَالُهُ إِذَا تَرَدَّىٰ",
        "verse_12": "إِنَّ عَلَيْنَا لَلْهُدَىٰ",
        "verse_13": "وَإِنَّ لَنَا لَلْآخِرَةَ وَالْأُولَىٰ",
        "verse_14": "فَأَنْذَرْتُكُمْ نَارًا تَلَظَّىٰ",
        "verse_15": "لَا يَصْلَاهَا إِلَّا الْأَشْقَى",
        "verse_16": "الَّذِي كَذَّبَ وَتَوَلَّىٰ",
        "verse_17": "وَسَيُجَنَّبُهَا الْأَتْقَى",
        "verse_18": "الَّذِي يُؤْتِي مَالَهُ يَتَزَكَّىٰ",
        "verse_19": "وَمَا لِأَحَدٍ عِنْدَهُ مِنْ نِعْمَةٍ تُجْزَىٰ",
        "verse_20": "إِلَّا ابْتِغَاءَ وَجْهِ رَبِّهِ الْأَعْلَىٰ",
        "verse_21": "وَلَسَوْفَ يَرْضَىٰ"
    },
    "count": 21,
    "juz": [
        {
            "index": "30",
            "verse": {
                "start": "verse_1",
                "end": "verse_21"
            }
        }
    ]
}

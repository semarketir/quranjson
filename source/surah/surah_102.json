    "name": "at-Takatsur",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "أَلْهَاكُمُ التَّكَاثُرُ",
        "verse_2": "حَتَّىٰ زُرْتُمُ الْمَقَابِرَ",
        "verse_3": "كَلَّا سَوْفَ تَعْلَمُونَ",
        "verse_4": "ثُمَّ كَلَّا سَوْفَ تَعْلَمُونَ",
        "verse_5": "كَلَّا لَوْ تَعْلَمُونَ عِلْمَ الْيَقِينِ",
        "verse_6": "لَتَرَوُنَّ الْجَحِيمَ",
        "verse_7": "ثُمَّ لَتَرَوُنَّهَا عَيْنَ الْيَقِينِ",
        "verse_8": "ثُمَّ لَتُسْأَلُنَّ يَوْمَئِذٍ عَنِ النَّعِيمِ"
    },

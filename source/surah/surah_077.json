{
    "index": "077",
    "name": "al-Mursalat",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "وَٱلْمُرْسَلَٰتِ عُرْفًۭا",
        "verse_2": "فَٱلْعَٰصِفَٰتِ عَصْفًۭا",
        "verse_3": "وَٱلنَّٰشِرَٰتِ نَشْرًۭا",
        "verse_4": "فَٱلْفَٰرِقَٰتِ فَرْقًۭا",
        "verse_5": "فَٱلْمُلْقِيَٰتِ ذِكْرًا",
        "verse_6": "عُذْرًا أَوْ نُذْرًا",
        "verse_7": "إِنَّمَا تُوعَدُونَ لَوَٰقِعٌۭ",
        "verse_8": "فَإِذَا ٱلنُّجُومُ طُمِسَتْ",
        "verse_9": "وَإِذَا ٱلسَّمَآءُ فُرِجَتْ",
        "verse_10": "وَإِذَا ٱلْجِبَالُ نُسِفَتْ",
        "verse_11": "وَإِذَا ٱلرُّسُلُ أُقِّتَتْ",
        "verse_12": "لِأَىِّ يَوْمٍ أُجِّلَتْ",
        "verse_13": "لِيَوْمِ ٱلْفَصْلِ",
        "verse_14": "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلْفَصْلِ",
        "verse_15": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
        "verse_16": "أَلَمْ نُهْلِكِ ٱلْأَوَّلِينَ",
        "verse_17": "ثُمَّ نُتْبِعُهُمُ ٱلْءَاخِرِينَ",
        "verse_18": "كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ",
        "verse_19": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
        "verse_20": "أَلَمْ نَخْلُقكُّم مِّن مَّآءٍۢ مَّهِينٍۢ",
        "verse_21": "فَجَعَلْنَٰهُ فِى قَرَارٍۢ مَّكِينٍ",
        "verse_22": "إِلَىٰ قَدَرٍۢ مَّعْلُومٍۢ",
        "verse_23": "فَقَدَرْنَا فَنِعْمَ ٱلْقَٰدِرُونَ",
        "verse_24": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
        "verse_25": "أَلَمْ نَجْعَلِ ٱلْأَرْضَ كِفَاتًا",
        "verse_26": "أَحْيَآءًۭ وَأَمْوَٰتًۭا",
        "verse_27": "وَجَعَلْنَا فِيهَا رَوَٰسِىَ شَٰمِخَٰتٍۢ وَأَسْقَيْنَٰكُم مَّآءًۭ فُرَاتًۭا",
        "verse_28": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
        "verse_29": "ٱنطَلِقُوٓا۟ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ",
        "verse_30": "ٱنطَلِقُوٓا۟ إِلَىٰ ظِلٍّۢ ذِى ثَلَٰثِ شُعَبٍۢ",
        "verse_31": "لَّا ظَلِيلٍۢ وَلَا يُغْنِى مِنَ ٱللَّهَبِ",
        "verse_32": "إِنَّهَا تَرْمِى بِشَرَرٍۢ كَٱلْقَصْرِ",
        "verse_33": "كَأَنَّهُۥ جِمَٰلَتٌۭ صُفْرٌۭ",
        "verse_34": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
        "verse_35": "هَٰذَا يَوْمُ لَا يَنطِقُونَ",
        "verse_36": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ",
        "verse_37": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
        "verse_38": "هَٰذَا يَوْمُ ٱلْفَصْلِ ۖ جَمَعْنَٰكُمْ وَٱلْأَوَّلِينَ",
        "verse_39": "فَإِن كَانَ لَكُمْ كَيْدٌۭ فَكِيدُونِ",
        "verse_40": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
        "verse_41": "إِنَّ ٱلْمُتَّقِينَ فِى ظِلَٰلٍۢ وَعُيُونٍۢ",
        "verse_42": "وَفَوَٰكِهَ مِمَّا يَشْتَهُونَ",
        "verse_43": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَا كُنتُمْ تَعْمَلُونَ",
        "verse_44": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
        "verse_45": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
        "verse_46": "كُلُوا۟ وَتَمَتَّعُوا۟ قَلِيلًا إِنَّكُم مُّجْرِمُونَ",
        "verse_47": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
        "verse_48": "وَإِذَا قِيلَ لَهُمُ ٱرْكَعُوا۟ لَا يَرْكَعُونَ",
        "verse_49": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
        "verse_50": "فَبِأَىِّ حَدِيثٍۭ بَعْدَهُۥ يُؤْمِنُونَ"
    },

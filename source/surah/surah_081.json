{
    "index": "081",
    "name": "at-Takwir",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "إِذَا ٱلشَّمْسُ كُوِّرَتْ",
        "verse_2": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتْ",
        "verse_3": "وَإِذَا ٱلْجِبَالُ سُيِّرَتْ",
        "verse_4": "وَإِذَا ٱلْعِشَارُ عُطِّلَتْ",
        "verse_5": "وَإِذَا ٱلْوُحُوشُ حُشِرَتْ",
        "verse_6": "وَإِذَا ٱلْبِحَارُ سُجِّرَتْ",
        "verse_7": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتْ",
        "verse_8": "وَإِذَا ٱلْمَوْءُۥدَةُ سُئِلَتْ",
        "verse_9": "بِأَىِّ ذَنۢبٍۢ قُتِلَتْ",
        "verse_10": "وَإِذَا ٱلصُّحُفُ نُشِرَتْ",
        "verse_11": "وَإِذَا ٱلسَّمَآءُ كُشِطَتْ",
        "verse_12": "وَإِذَا ٱلْجَحِيمُ سُعِّرَتْ",
        "verse_13": "وَإِذَا ٱلْجَنَّةُ أُزْلِفَتْ",
        "verse_14": "عَلِمَتْ نَفْسٌۭ مَّآ أَحْضَرَتْ",
        "verse_15": "فَلَآ أُقْسِمُ بِٱلْخُنَّسِ",
        "verse_16": "ٱلْجَوَارِ ٱلْكُنَّسِ",
        "verse_17": "وَٱلَّيْلِ إِذَا عَسْعَسَ",
        "verse_18": "وَٱلصُّبْحِ إِذَا تَنَفَّسَ",
        "verse_19": "إِنَّهُۥ لَقَوْلُ رَسُولٍۢ كَرِيمٍۢ",
        "verse_20": "ذِى قُوَّةٍ عِندَ ذِى ٱلْعَرْشِ مَكِينٍۢ",
        "verse_21": "مُّطَاعٍۢ ثَمَّ أَمِينٍۢ",
        "verse_22": "وَمَا صَاحِبُكُم بِمَجْنُونٍۢ",
        "verse_23": "وَلَقَدْ رَءَاهُ بِٱلْأُفُقِ ٱلْمُبِينِ",
        "verse_24": "وَمَا هُوَ عَلَى ٱلْغَيْبِ بِضَنِينٍۢ",
        "verse_25": "وَمَا هُوَ بِقَوْلِ شَيْطَٰنٍۢ رَّجِيمٍۢ",
        "verse_26": "فَأَيْنَ تَذْهَبُونَ",
        "verse_27": "إِنْ هُوَ إِلَّا ذِكْرٌۭ لِّلْعَٰلَمِينَ",
        "verse_28": "لِمَن شَآءَ مِنكُمْ أَن يَسْتَقِيمَ",
        "verse_29": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلْعَٰلَمِينَ"
    },
    "count": 29,

{
    "index": "087",
    "name": "al-A'la",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى",
        "verse_2": "ٱلَّذِى خَلَقَ فَسَوَّىٰ",
        "verse_3": "وَٱلَّذِى قَدَّرَ فَهَدَىٰ",
        "verse_4": "وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ",
        "verse_5": "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ",
        "verse_6": "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ",
        "verse_7": "إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ",
        "verse_8": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ",
        "verse_9": "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ",
        "verse_10": "سَيَذَّكَّرُ مَن يَخْشَىٰ",
        "verse_11": "وَيَتَجَنَّبُهَا ٱلْأَشْقَى",
        "verse_12": "ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ",
        "verse_13": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
        "verse_14": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ",
        "verse_15": "وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ",
        "verse_16": "بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
        "verse_17": "وَٱلْءَاخِرَةُ خَيْرٌۭ وَأَبْقَىٰٓ",
        "verse_18": "إِنَّ هَٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ",
        "verse_19": "صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ"
    },
    "count": 19,
    "juz": [

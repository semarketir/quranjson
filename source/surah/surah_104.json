{
    "index": "104",
    "name": "al-Humazah",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "وَيْلٌۭ لِّكُلِّ هُمَزَةٍۢ لُّمَزَةٍ",
        "verse_2": "ٱلَّذِى جَمَعَ مَالًۭا وَعَدَّدَهُۥ",
        "verse_3": "يَحْسَبُ أَنَّ مَالَهُۥٓ أَخْلَدَهُۥ",
        "verse_4": "كَلَّا ۖ لَيُنۢبَذَنَّ فِى ٱلْحُطَمَةِ",
        "verse_5": "وَمَآ أَدْرَىٰكَ مَا ٱلْحُطَمَةُ",
        "verse_6": "نَارُ ٱللَّهِ ٱلْمُوقَدَةُ",
        "verse_7": "ٱلَّتِى تَطَّلِعُ عَلَى ٱلْأَفْـِٔدَةِ",
        "verse_8": "إِنَّهَا عَلَيْهِم مُّؤْصَدَةٌۭ",
        "verse_9": "فِى عَمَدٍۢ مُّمَدَّدَةٍۭ"
    },
    "count": 9,

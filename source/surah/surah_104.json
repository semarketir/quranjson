{
    "index": "104",
    "name": "al-Humazah",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "وَيْلٌ لِكُلِّ هُمَزَةٍ لُمَزَةٍ",
        "verse_2": "الَّذِي جَمَعَ مَالًا وَعَدَّدَهُ",
        "verse_3": "يَحْسَبُ أَنَّ مَالَهُ أَخْلَدَهُ",
        "verse_4": "كَلَّا ۖ لَيُنْبَذَنَّ فِي الْحُطَمَةِ",
        "verse_5": "وَمَا أَدْرَاكَ مَا الْحُطَمَةُ",
        "verse_6": "نَارُ اللَّهِ الْمُوقَدَةُ",
        "verse_7": "الَّتِي تَطَّلِعُ عَلَى الْأَفْئِدَةِ",
        "verse_8": "إِنَّهَا عَلَيْهِمْ مُؤْصَدَةٌ",
        "verse_9": "فِي عَمَدٍ مُمَدَّدَةٍ"
    },

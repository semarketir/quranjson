{
    "index": "090",
    "name": "al-Balad",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "لَا أُقْسِمُ بِهَٰذَا الْبَلَدِ",
        "verse_2": "وَأَنْتَ حِلٌّ بِهَٰذَا الْبَلَدِ",
        "verse_3": "وَوَالِدٍ وَمَا وَلَدَ",
        "verse_4": "لَقَدْ خَلَقْنَا الْإِنْسَانَ فِي كَبَدٍ",
        "verse_5": "أَيَحْسَبُ أَنْ لَنْ يَقْدِرَ عَلَيْهِ أَحَدٌ",
        "verse_6": "يَقُولُ أَهْلَكْتُ مَالًا لُبَدًا",
        "verse_7": "أَيَحْسَبُ أَنْ لَمْ يَرَهُ أَحَدٌ",
        "verse_8": "أَلَمْ نَجْعَلْ لَهُ عَيْنَيْنِ",
        "verse_9": "وَلِسَانًا وَشَفَتَيْنِ",
        "verse_10": "وَهَدَيْنَاهُ النَّجْدَيْنِ",
        "verse_11": "فَلَا اقْتَحَمَ الْعَقَبَةَ",
        "verse_12": "وَمَا أَدْرَاكَ مَا الْعَقَبَةُ",
        "verse_13": "فَكُّ رَقَبَةٍ",
        "verse_14": "أَوْ إِطْعَامٌ فِي يَوْمٍ ذِي مَسْغَبَةٍ",
        "verse_15": "يَتِيمًا ذَا مَقْرَبَةٍ",
        "verse_16": "أَوْ مِسْكِينًا ذَا مَتْرَبَةٍ",
        "verse_17": "ثُمَّ كَانَ مِنَ الَّذِينَ آمَنُوا وَتَوَاصَوْا بِالصَّبْرِ وَتَوَاصَوْا بِالْمَرْحَمَةِ",
        "verse_18": "أُولَٰئِكَ أَصْحَابُ الْمَيْمَنَةِ",
        "verse_19": "وَالَّذِينَ كَفَرُوا بِآيَاتِنَا هُمْ أَصْحَابُ الْمَشْأَمَةِ",
        "verse_20": "عَلَيْهِمْ نَارٌ مُؤْصَدَةٌ"
    },
    "count": 20,
    "juz": [

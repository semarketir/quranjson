{
    "index": "096",
    "name": "al-'Alaq",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "اقْرَأْ بِاسْمِ رَبِّكَ الَّذِي خَلَقَ",
        "verse_2": "خَلَقَ الْإِنْسَانَ مِنْ عَلَقٍ",
        "verse_3": "اقْرَأْ وَرَبُّكَ الْأَكْرَمُ",
        "verse_4": "الَّذِي عَلَّمَ بِالْقَلَمِ",
        "verse_5": "عَلَّمَ الْإِنْسَانَ مَا لَمْ يَعْلَمْ",
        "verse_6": "كَلَّا إِنَّ الْإِنْسَانَ لَيَطْغَىٰ",
        "verse_7": "أَنْ رَآهُ اسْتَغْنَىٰ",
        "verse_8": "إِنَّ إِلَىٰ رَبِّكَ الرُّجْعَىٰ",
        "verse_9": "أَرَأَيْتَ الَّذِي يَنْهَىٰ",
        "verse_10": "عَبْدًا إِذَا صَلَّىٰ",
        "verse_11": "أَرَأَيْتَ إِنْ كَانَ عَلَى الْهُدَىٰ",
        "verse_12": "أَوْ أَمَرَ بِالتَّقْوَىٰ",
        "verse_13": "أَرَأَيْتَ إِنْ كَذَّبَ وَتَوَلَّىٰ",
        "verse_14": "أَلَمْ يَعْلَمْ بِأَنَّ اللَّهَ يَرَىٰ",
        "verse_15": "كَلَّا لَئِنْ لَمْ يَنْتَهِ لَنَسْفَعًا بِالنَّاصِيَةِ",
        "verse_16": "نَاصِيَةٍ كَاذِبَةٍ خَاطِئَةٍ",
        "verse_17": "فَلْيَدْعُ نَادِيَهُ",
        "verse_18": "سَنَدْعُ الزَّبَانِيَةَ",
        "verse_19": "كَلَّا لَا تُطِعْهُ وَاسْجُدْ وَاقْتَرِبْ ۩"
    },

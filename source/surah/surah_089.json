{
    "index": "089",
    "name": "al-Fajr",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "وَٱلْفَجْرِ",
        "verse_2": "وَلَيَالٍ عَشْرٍۢ",
        "verse_3": "وَٱلشَّفْعِ وَٱلْوَتْرِ",
        "verse_4": "وَٱلَّيْلِ إِذَا يَسْرِ",
        "verse_5": "هَلْ فِى ذَٰلِكَ قَسَمٌۭ لِّذِى حِجْرٍ",
        "verse_6": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ",
        "verse_7": "إِرَمَ ذَاتِ ٱلْعِمَادِ",
        "verse_8": "ٱلَّتِى لَمْ يُخْلَقْ مِثْلُهَا فِى ٱلْبِلَٰدِ",
        "verse_9": "وَثَمُودَ ٱلَّذِينَ جَابُوا۟ ٱلصَّخْرَ بِٱلْوَادِ",
        "verse_10": "وَفِرْعَوْنَ ذِى ٱلْأَوْتَادِ",
        "verse_11": "ٱلَّذِينَ طَغَوْا۟ فِى ٱلْبِلَٰدِ",
        "verse_12": "فَأَكْثَرُوا۟ فِيهَا ٱلْفَسَادَ",
        "verse_13": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ",
        "verse_14": "إِنَّ رَبَّكَ لَبِٱلْمِرْصَادِ",
        "verse_15": "فَأَمَّا ٱلْإِنسَٰنُ إِذَا مَا ٱبْتَلَىٰهُ رَبُّهُۥ فَأَكْرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّىٓ أَكْرَمَنِ",
        "verse_16": "وَأَمَّآ إِذَا مَا ٱبْتَلَىٰهُ فَقَدَرَ عَلَيْهِ رِزْقَهُۥ فَيَقُولُ رَبِّىٓ أَهَٰنَنِ",
        "verse_17": "كَلَّا ۖ بَل لَّا تُكْرِمُونَ ٱلْيَتِيمَ",
        "verse_18": "وَلَا تَحَٰٓضُّونَ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ",
        "verse_19": "وَتَأْكُلُونَ ٱلتُّرَاثَ أَكْلًۭا لَّمًّۭا",
        "verse_20": "وَتُحِبُّونَ ٱلْمَالَ حُبًّۭا جَمًّۭا",
        "verse_21": "كَلَّآ إِذَا دُكَّتِ ٱلْأَرْضُ دَكًّۭا دَكًّۭا",
        "verse_22": "وَجَآءَ رَبُّكَ وَٱلْمَلَكُ صَفًّۭا صَفًّۭا",
        "verse_23": "وَجِا۟ىٓءَ يَوْمَئِذٍۭ بِجَهَنَّمَ ۚ يَوْمَئِذٍۢ يَتَذَكَّرُ ٱلْإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكْرَىٰ",
        "verse_24": "يَقُولُ يَٰلَيْتَنِى قَدَّمْتُ لِحَيَاتِى",
        "verse_25": "فَيَوْمَئِذٍۢ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٌۭ",
        "verse_26": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٌۭ",
        "verse_27": "يَٰٓأَيَّتُهَا ٱلنَّفْسُ ٱلْمُطْمَئِنَّةُ",
        "verse_28": "ٱرْجِعِىٓ إِلَىٰ رَبِّكِ رَاضِيَةًۭ مَّرْضِيَّةًۭ",
        "verse_29": "فَٱدْخُلِى فِى عِبَٰدِى",
        "verse_30": "وَٱدْخُلِى جَنَّتِى"
    },

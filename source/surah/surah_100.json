    "name": "al-'Adiyat ",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "وَٱلْعَٰدِيَٰتِ ضَبْحًۭا",
        "verse_2": "فَٱلْمُورِيَٰتِ قَدْحًۭا",
        "verse_3": "فَٱلْمُغِيرَٰتِ صُبْحًۭا",
        "verse_4": "فَأَثَرْنَ بِهِۦ نَقْعًۭا",
        "verse_5": "فَوَسَطْنَ بِهِۦ جَمْعًا",
        "verse_6": "إِنَّ ٱلْإِنسَٰنَ لِرَبِّهِۦ لَكَنُودٌۭ",
        "verse_7": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٌۭ",
        "verse_8": "وَإِنَّهُۥ لِحُبِّ ٱلْخَيْرِ لَشَدِيدٌ",
        "verse_9": "۞ أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِى ٱلْقُبُورِ",
        "verse_10": "وَحُصِّلَ مَا فِى ٱلصُّدُورِ",
        "verse_11": "إِنَّ رَبَّهُم بِهِمْ يَوْمَئِذٍۢ لَّخَبِيرٌۢ"
    },

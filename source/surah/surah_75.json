{
    "index": "075",
    "name": "al-Qiyamah",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "لَا أُقْسِمُ بِيَوْمِ الْقِيَامَةِ",
        "verse_2": "وَلَا أُقْسِمُ بِالنَّفْسِ اللَّوَّامَةِ",
        "verse_3": "أَيَحْسَبُ الْإِنْسَانُ أَلَّنْ نَجْمَعَ عِظَامَهُ",
        "verse_4": "بَلَىٰ قَادِرِينَ عَلَىٰ أَنْ نُسَوِّيَ بَنَانَهُ",
        "verse_5": "بَلْ يُرِيدُ الْإِنْسَانُ لِيَفْجُرَ أَمَامَهُ",
        "verse_6": "يَسْأَلُ أَيَّانَ يَوْمُ الْقِيَامَةِ",
        "verse_7": "فَإِذَا بَرِقَ الْبَصَرُ",
        "verse_8": "وَخَسَفَ الْقَمَرُ",
        "verse_9": "وَجُمِعَ الشَّمْسُ وَالْقَمَرُ",
        "verse_10": "يَقُولُ الْإِنْسَانُ يَوْمَئِذٍ أَيْنَ الْمَفَرُّ",
        "verse_11": "كَلَّا لَا وَزَرَ",
        "verse_12": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ الْمُسْتَقَرُّ",
        "verse_13": "يُنَبَّأُ الْإِنْسَانُ يَوْمَئِذٍ بِمَا قَدَّمَ وَأَخَّرَ",
        "verse_14": "بَلِ الْإِنْسَانُ عَلَىٰ نَفْسِهِ بَصِيرَةٌ",
        "verse_15": "وَلَوْ أَلْقَىٰ مَعَاذِيرَهُ",
        "verse_16": "لَا تُحَرِّكْ بِهِ لِسَانَكَ لِتَعْجَلَ بِهِ",
        "verse_17": "إِنَّ عَلَيْنَا جَمْعَهُ وَقُرْآنَهُ",
        "verse_18": "فَإِذَا قَرَأْنَاهُ فَاتَّبِعْ قُرْآنَهُ",
        "verse_19": "ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُ",
        "verse_20": "كَلَّا بَلْ تُحِبُّونَ الْعَاجِلَةَ",
        "verse_21": "وَتَذَرُونَ الْآخِرَةَ",
        "verse_22": "وُجُوهٌ يَوْمَئِذٍ نَاضِرَةٌ",
        "verse_23": "إِلَىٰ رَبِّهَا نَاظِرَةٌ",
        "verse_24": "وَوُجُوهٌ يَوْمَئِذٍ بَاسِرَةٌ",
        "verse_25": "تَظُنُّ أَنْ يُفْعَلَ بِهَا فَاقِرَةٌ",
        "verse_26": "كَلَّا إِذَا بَلَغَتِ التَّرَاقِيَ",
        "verse_27": "وَقِيلَ مَنْ ۜ رَاقٍ",
        "verse_28": "وَظَنَّ أَنَّهُ الْفِرَاقُ",
        "verse_29": "وَالْتَفَّتِ السَّاقُ بِالسَّاقِ",
        "verse_30": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ الْمَسَاقُ",
        "verse_31": "فَلَا صَدَّقَ وَلَا صَلَّىٰ",
        "verse_32": "وَلَٰكِنْ كَذَّبَ وَتَوَلَّىٰ",
        "verse_33": "ثُمَّ ذَهَبَ إِلَىٰ أَهْلِهِ يَتَمَطَّىٰ",
        "verse_34": "أَوْلَىٰ لَكَ فَأَوْلَىٰ",
        "verse_35": "ثُمَّ أَوْلَىٰ لَكَ فَأَوْلَىٰ",
        "verse_36": "أَيَحْسَبُ الْإِنْسَانُ أَنْ يُتْرَكَ سُدًى",
        "verse_37": "أَلَمْ يَكُ نُطْفَةً مِنْ مَنِيٍّ يُمْنَىٰ",
        "verse_38": "ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوَّىٰ",
        "verse_39": "فَجَعَلَ مِنْهُ الزَّوْجَيْنِ الذَّكَرَ وَالْأُنْثَىٰ",
        "verse_40": "أَلَيْسَ ذَٰلِكَ بِقَادِرٍ عَلَىٰ أَنْ يُحْيِيَ الْمَوْتَىٰ"
    },

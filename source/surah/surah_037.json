{
    "index": "037",
    "name": "As-Saffat",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "وَٱلصَّٰٓفَّٰتِ صَفًّۭا",
        "verse_2": "فَٱلزَّٰجِرَٰتِ زَجْرًۭا",
        "verse_3": "فَٱلتَّٰلِيَٰتِ ذِكْرًا",
        "verse_4": "إِنَّ إِلَٰهَكُمْ لَوَٰحِدٌۭ",
        "verse_5": "رَّبُّ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا وَرَبُّ ٱلْمَشَٰرِقِ",
        "verse_6": "إِنَّا زَيَّنَّا ٱلسَّمَآءَ ٱلدُّنْيَا بِزِينَةٍ ٱلْكَوَاكِبِ",
        "verse_7": "وَحِفْظًۭا مِّن كُلِّ شَيْطَٰنٍۢ مَّارِدٍۢ",
        "verse_8": "لَّا يَسَّمَّعُونَ إِلَى ٱلْمَلَإِ ٱلْأَعْلَىٰ وَيُقْذَفُونَ مِن كُلِّ جَانِبٍۢ",
        "verse_9": "دُحُورًۭا ۖ وَلَهُمْ عَذَابٌۭ وَاصِبٌ",
        "verse_10": "إِلَّا مَنْ خَطِفَ ٱلْخَطْفَةَ فَأَتْبَعَهُۥ شِهَابٌۭ ثَاقِبٌۭ",
        "verse_11": "فَٱسْتَفْتِهِمْ أَهُمْ أَشَدُّ خَلْقًا أَم مَّنْ خَلَقْنَآ ۚ إِنَّا خَلَقْنَٰهُم مِّن طِينٍۢ لَّازِبٍۭ",
        "verse_12": "بَلْ عَجِبْتَ وَيَسْخَرُونَ",
        "verse_13": "وَإِذَا ذُكِّرُوا۟ لَا يَذْكُرُونَ",
        "verse_14": "وَإِذَا رَأَوْا۟ ءَايَةًۭ يَسْتَسْخِرُونَ",
        "verse_15": "وَقَالُوٓا۟ إِنْ هَٰذَآ إِلَّا سِحْرٌۭ مُّبِينٌ",
        "verse_16": "أَءِذَا مِتْنَا وَكُنَّا تُرَابًۭا وَعِظَٰمًا أَءِنَّا لَمَبْعُوثُونَ",
        "verse_17": "أَوَءَابَآؤُنَا ٱلْأَوَّلُونَ",
        "verse_18": "قُلْ نَعَمْ وَأَنتُمْ دَٰخِرُونَ",
        "verse_19": "فَإِنَّمَا هِىَ زَجْرَةٌۭ وَٰحِدَةٌۭ فَإِذَا هُمْ يَنظُرُونَ",
        "verse_20": "وَقَالُوا۟ يَٰوَيْلَنَا هَٰذَا يَوْمُ ٱلدِّينِ",
        "verse_21": "هَٰذَا يَوْمُ ٱلْفَصْلِ ٱلَّذِى كُنتُم بِهِۦ تُكَذِّبُونَ",
        "verse_22": "۞ ٱحْشُرُوا۟ ٱلَّذِينَ ظَلَمُوا۟ وَأَزْوَٰجَهُمْ وَمَا كَانُوا۟ يَعْبُدُونَ",
        "verse_23": "مِن دُونِ ٱللَّهِ فَٱهْدُوهُمْ إِلَىٰ صِرَٰطِ ٱلْجَحِيمِ",
        "verse_24": "وَقِفُوهُمْ ۖ إِنَّهُم مَّسْـُٔولُونَ",
        "verse_25": "مَا لَكُمْ لَا تَنَاصَرُونَ",
        "verse_26": "بَلْ هُمُ ٱلْيَوْمَ مُسْتَسْلِمُونَ",
        "verse_27": "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍۢ يَتَسَآءَلُونَ",
        "verse_28": "قَالُوٓا۟ إِنَّكُمْ كُنتُمْ تَأْتُونَنَا عَنِ ٱلْيَمِينِ",
        "verse_29": "قَالُوا۟ بَل لَّمْ تَكُونُوا۟ مُؤْمِنِينَ",
        "verse_30": "وَمَا كَانَ لَنَا عَلَيْكُم مِّن سُلْطَٰنٍۭ ۖ بَلْ كُنتُمْ قَوْمًۭا طَٰغِينَ",
        "verse_31": "فَحَقَّ عَلَيْنَا قَوْلُ رَبِّنَآ ۖ إِنَّا لَذَآئِقُونَ",
        "verse_32": "فَأَغْوَيْنَٰكُمْ إِنَّا كُنَّا غَٰوِينَ",
        "verse_33": "فَإِنَّهُمْ يَوْمَئِذٍۢ فِى ٱلْعَذَابِ مُشْتَرِكُونَ",
        "verse_34": "إِنَّا كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ",
        "verse_35": "إِنَّهُمْ كَانُوٓا۟ إِذَا قِيلَ لَهُمْ لَآ إِلَٰهَ إِلَّا ٱللَّهُ يَسْتَكْبِرُونَ",
        "verse_36": "وَيَقُولُونَ أَئِنَّا لَتَارِكُوٓا۟ ءَالِهَتِنَا لِشَاعِرٍۢ مَّجْنُونٍۭ",
        "verse_37": "بَلْ جَآءَ بِٱلْحَقِّ وَصَدَّقَ ٱلْمُرْسَلِينَ",
        "verse_38": "إِنَّكُمْ لَذَآئِقُوا۟ ٱلْعَذَابِ ٱلْأَلِيمِ",
        "verse_39": "وَمَا تُجْزَوْنَ إِلَّا مَا كُنتُمْ تَعْمَلُونَ",
        "verse_40": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
        "verse_41": "أُو۟لَٰٓئِكَ لَهُمْ رِزْقٌۭ مَّعْلُومٌۭ",
        "verse_42": "فَوَٰكِهُ ۖ وَهُم مُّكْرَمُونَ",
        "verse_43": "فِى جَنَّٰتِ ٱلنَّعِيمِ",
        "verse_44": "عَلَىٰ سُرُرٍۢ مُّتَقَٰبِلِينَ",
        "verse_45": "يُطَافُ عَلَيْهِم بِكَأْسٍۢ مِّن مَّعِينٍۭ",
        "verse_46": "بَيْضَآءَ لَذَّةٍۢ لِّلشَّٰرِبِينَ",
        "verse_47": "لَا فِيهَا غَوْلٌۭ وَلَا هُمْ عَنْهَا يُنزَفُونَ",
        "verse_48": "وَعِندَهُمْ قَٰصِرَٰتُ ٱلطَّرْفِ عِينٌۭ",
        "verse_49": "كَأَنَّهُنَّ بَيْضٌۭ مَّكْنُونٌۭ",
        "verse_50": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍۢ يَتَسَآءَلُونَ",
        "verse_51": "قَالَ قَآئِلٌۭ مِّنْهُمْ إِنِّى كَانَ لِى قَرِينٌۭ",
        "verse_52": "يَقُولُ أَءِنَّكَ لَمِنَ ٱلْمُصَدِّقِينَ",
        "verse_53": "أَءِذَا مِتْنَا وَكُنَّا تُرَابًۭا وَعِظَٰمًا أَءِنَّا لَمَدِينُونَ",
        "verse_54": "قَالَ هَلْ أَنتُم مُّطَّلِعُونَ",
        "verse_55": "فَٱطَّلَعَ فَرَءَاهُ فِى سَوَآءِ ٱلْجَحِيمِ",
        "verse_56": "قَالَ تَٱللَّهِ إِن كِدتَّ لَتُرْدِينِ",
        "verse_57": "وَلَوْلَا نِعْمَةُ رَبِّى لَكُنتُ مِنَ ٱلْمُحْضَرِينَ",
        "verse_58": "أَفَمَا نَحْنُ بِمَيِّتِينَ",
        "verse_59": "إِلَّا مَوْتَتَنَا ٱلْأُولَىٰ وَمَا نَحْنُ بِمُعَذَّبِينَ",
        "verse_60": "إِنَّ هَٰذَا لَهُوَ ٱلْفَوْزُ ٱلْعَظِيمُ",
        "verse_61": "لِمِثْلِ هَٰذَا فَلْيَعْمَلِ ٱلْعَٰمِلُونَ",
        "verse_62": "أَذَٰلِكَ خَيْرٌۭ نُّزُلًا أَمْ شَجَرَةُ ٱلزَّقُّومِ",
        "verse_63": "إِنَّا جَعَلْنَٰهَا فِتْنَةًۭ لِّلظَّٰلِمِينَ",
        "verse_64": "إِنَّهَا شَجَرَةٌۭ تَخْرُجُ فِىٓ أَصْلِ ٱلْجَحِيمِ",
        "verse_65": "طَلْعُهَا كَأَنَّهُۥ رُءُوسُ ٱلشَّيَٰطِينِ",
        "verse_66": "فَإِنَّهُمْ لَءَاكِلُونَ مِنْهَا فَمَالِـُٔونَ مِنْهَا ٱلْبُطُونَ",
        "verse_67": "ثُمَّ إِنَّ لَهُمْ عَلَيْهَا لَشَوْبًۭا مِّنْ حَمِيمٍۢ",
        "verse_68": "ثُمَّ إِنَّ مَرْجِعَهُمْ لَإِلَى ٱلْجَحِيمِ",
        "verse_69": "إِنَّهُمْ أَلْفَوْا۟ ءَابَآءَهُمْ ضَآلِّينَ",
        "verse_70": "فَهُمْ عَلَىٰٓ ءَاثَٰرِهِمْ يُهْرَعُونَ",
        "verse_71": "وَلَقَدْ ضَلَّ قَبْلَهُمْ أَكْثَرُ ٱلْأَوَّلِينَ",
        "verse_72": "وَلَقَدْ أَرْسَلْنَا فِيهِم مُّنذِرِينَ",
        "verse_73": "فَٱنظُرْ كَيْفَ كَانَ عَٰقِبَةُ ٱلْمُنذَرِينَ",
        "verse_74": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
        "verse_75": "وَلَقَدْ نَادَىٰنَا نُوحٌۭ فَلَنِعْمَ ٱلْمُجِيبُونَ",
        "verse_76": "وَنَجَّيْنَٰهُ وَأَهْلَهُۥ مِنَ ٱلْكَرْبِ ٱلْعَظِيمِ",
        "verse_77": "وَجَعَلْنَا ذُرِّيَّتَهُۥ هُمُ ٱلْبَاقِينَ",
        "verse_78": "وَتَرَكْنَا عَلَيْهِ فِى ٱلْءَاخِرِينَ",
        "verse_79": "سَلَٰمٌ عَلَىٰ نُوحٍۢ فِى ٱلْعَٰلَمِينَ",
        "verse_80": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
        "verse_81": "إِنَّهُۥ مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ",
        "verse_82": "ثُمَّ أَغْرَقْنَا ٱلْءَاخَرِينَ",
        "verse_83": "۞ وَإِنَّ مِن شِيعَتِهِۦ لَإِبْرَٰهِيمَ",
        "verse_84": "إِذْ جَآءَ رَبَّهُۥ بِقَلْبٍۢ سَلِيمٍ",
        "verse_85": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِۦ مَاذَا تَعْبُدُونَ",
        "verse_86": "أَئِفْكًا ءَالِهَةًۭ دُونَ ٱللَّهِ تُرِيدُونَ",
        "verse_87": "فَمَا ظَنُّكُم بِرَبِّ ٱلْعَٰلَمِينَ",
        "verse_88": "فَنَظَرَ نَظْرَةًۭ فِى ٱلنُّجُومِ",
        "verse_89": "فَقَالَ إِنِّى سَقِيمٌۭ",
        "verse_90": "فَتَوَلَّوْا۟ عَنْهُ مُدْبِرِينَ",
        "verse_91": "فَرَاغَ إِلَىٰٓ ءَالِهَتِهِمْ فَقَالَ أَلَا تَأْكُلُونَ",
        "verse_92": "مَا لَكُمْ لَا تَنطِقُونَ",
        "verse_93": "فَرَاغَ عَلَيْهِمْ ضَرْبًۢا بِٱلْيَمِينِ",
        "verse_94": "فَأَقْبَلُوٓا۟ إِلَيْهِ يَزِفُّونَ",
        "verse_95": "قَالَ أَتَعْبُدُونَ مَا تَنْحِتُونَ",
        "verse_96": "وَٱللَّهُ خَلَقَكُمْ وَمَا تَعْمَلُونَ",
        "verse_97": "قَالُوا۟ ٱبْنُوا۟ لَهُۥ بُنْيَٰنًۭا فَأَلْقُوهُ فِى ٱلْجَحِيمِ",
        "verse_98": "فَأَرَادُوا۟ بِهِۦ كَيْدًۭا فَجَعَلْنَٰهُمُ ٱلْأَسْفَلِينَ",
        "verse_99": "وَقَالَ إِنِّى ذَاهِبٌ إِلَىٰ رَبِّى سَيَهْدِينِ",
        "verse_100": "رَبِّ هَبْ لِى مِنَ ٱلصَّٰلِحِينَ",
        "verse_101": "فَبَشَّرْنَٰهُ بِغُلَٰمٍ حَلِيمٍۢ",
        "verse_102": "فَلَمَّا بَلَغَ مَعَهُ ٱلسَّعْىَ قَالَ يَٰبُنَىَّ إِنِّىٓ أَرَىٰ فِى ٱلْمَنَامِ أَنِّىٓ أَذْبَحُكَ فَٱنظُرْ مَاذَا تَرَىٰ ۚ قَالَ يَٰٓأَبَتِ ٱفْعَلْ مَا تُؤْمَرُ ۖ سَتَجِدُنِىٓ إِن شَآءَ ٱللَّهُ مِنَ ٱلصَّٰبِرِينَ",
        "verse_103": "فَلَمَّآ أَسْلَمَا وَتَلَّهُۥ لِلْجَبِينِ",
        "verse_104": "وَنَٰدَيْنَٰهُ أَن يَٰٓإِبْرَٰهِيمُ",
        "verse_105": "قَدْ صَدَّقْتَ ٱلرُّءْيَآ ۚ إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
        "verse_106": "إِنَّ هَٰذَا لَهُوَ ٱلْبَلَٰٓؤُا۟ ٱلْمُبِينُ",
        "verse_107": "وَفَدَيْنَٰهُ بِذِبْحٍ عَظِيمٍۢ",
        "verse_108": "وَتَرَكْنَا عَلَيْهِ فِى ٱلْءَاخِرِينَ",
        "verse_109": "سَلَٰمٌ عَلَىٰٓ إِبْرَٰهِيمَ",
        "verse_110": "كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
        "verse_111": "إِنَّهُۥ مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ",
        "verse_112": "وَبَشَّرْنَٰهُ بِإِسْحَٰقَ نَبِيًّۭا مِّنَ ٱلصَّٰلِحِينَ",
        "verse_113": "وَبَٰرَكْنَا عَلَيْهِ وَعَلَىٰٓ إِسْحَٰقَ ۚ وَمِن ذُرِّيَّتِهِمَا مُحْسِنٌۭ وَظَالِمٌۭ لِّنَفْسِهِۦ مُبِينٌۭ",
        "verse_114": "وَلَقَدْ مَنَنَّا عَلَىٰ مُوسَىٰ وَهَٰرُونَ",
        "verse_115": "وَنَجَّيْنَٰهُمَا وَقَوْمَهُمَا مِنَ ٱلْكَرْبِ ٱلْعَظِيمِ",
        "verse_116": "وَنَصَرْنَٰهُمْ فَكَانُوا۟ هُمُ ٱلْغَٰلِبِينَ",
        "verse_117": "وَءَاتَيْنَٰهُمَا ٱلْكِتَٰبَ ٱلْمُسْتَبِينَ",
        "verse_118": "وَهَدَيْنَٰهُمَا ٱلصِّرَٰطَ ٱلْمُسْتَقِيمَ",
        "verse_119": "وَتَرَكْنَا عَلَيْهِمَا فِى ٱلْءَاخِرِينَ",
        "verse_120": "سَلَٰمٌ عَلَىٰ مُوسَىٰ وَهَٰرُونَ",
        "verse_121": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
        "verse_122": "إِنَّهُمَا مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ",
        "verse_123": "وَإِنَّ إِلْيَاسَ لَمِنَ ٱلْمُرْسَلِينَ",
        "verse_124": "إِذْ قَالَ لِقَوْمِهِۦٓ أَلَا تَتَّقُونَ",
        "verse_125": "أَتَدْعُونَ بَعْلًۭا وَتَذَرُونَ أَحْسَنَ ٱلْخَٰلِقِينَ",
        "verse_126": "ٱللَّهَ رَبَّكُمْ وَرَبَّ ءَابَآئِكُمُ ٱلْأَوَّلِينَ",
        "verse_127": "فَكَذَّبُوهُ فَإِنَّهُمْ لَمُحْضَرُونَ",
        "verse_128": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
        "verse_129": "وَتَرَكْنَا عَلَيْهِ فِى ٱلْءَاخِرِينَ",
        "verse_130": "سَلَٰمٌ عَلَىٰٓ إِلْ يَاسِينَ",
        "verse_131": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
        "verse_132": "إِنَّهُۥ مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ",
        "verse_133": "وَإِنَّ لُوطًۭا لَّمِنَ ٱلْمُرْسَلِينَ",
        "verse_134": "إِذْ نَجَّيْنَٰهُ وَأَهْلَهُۥٓ أَجْمَعِينَ",
        "verse_135": "إِلَّا عَجُوزًۭا فِى ٱلْغَٰبِرِينَ",
        "verse_136": "ثُمَّ دَمَّرْنَا ٱلْءَاخَرِينَ",
        "verse_137": "وَإِنَّكُمْ لَتَمُرُّونَ عَلَيْهِم مُّصْبِحِينَ",
        "verse_138": "وَبِٱلَّيْلِ ۗ أَفَلَا تَعْقِلُونَ",
        "verse_139": "وَإِنَّ يُونُسَ لَمِنَ ٱلْمُرْسَلِينَ",
        "verse_140": "إِذْ أَبَقَ إِلَى ٱلْفُلْكِ ٱلْمَشْحُونِ",
        "verse_141": "فَسَاهَمَ فَكَانَ مِنَ ٱلْمُدْحَضِينَ",
        "verse_142": "فَٱلْتَقَمَهُ ٱلْحُوتُ وَهُوَ مُلِيمٌۭ",
        "verse_143": "فَلَوْلَآ أَنَّهُۥ كَانَ مِنَ ٱلْمُسَبِّحِينَ",
        "verse_144": "لَلَبِثَ فِى بَطْنِهِۦٓ إِلَىٰ يَوْمِ يُبْعَثُونَ",
        "verse_145": "۞ فَنَبَذْنَٰهُ بِٱلْعَرَآءِ وَهُوَ سَقِيمٌۭ",
        "verse_146": "وَأَنۢبَتْنَا عَلَيْهِ شَجَرَةًۭ مِّن يَقْطِينٍۢ",
        "verse_147": "وَأَرْسَلْنَٰهُ إِلَىٰ مِا۟ئَةِ أَلْفٍ أَوْ يَزِيدُونَ",
        "verse_148": "فَـَٔامَنُوا۟ فَمَتَّعْنَٰهُمْ إِلَىٰ حِينٍۢ",
        "verse_149": "فَٱسْتَفْتِهِمْ أَلِرَبِّكَ ٱلْبَنَاتُ وَلَهُمُ ٱلْبَنُونَ",
        "verse_150": "أَمْ خَلَقْنَا ٱلْمَلَٰٓئِكَةَ إِنَٰثًۭا وَهُمْ شَٰهِدُونَ",
        "verse_151": "أَلَآ إِنَّهُم مِّنْ إِفْكِهِمْ لَيَقُولُونَ",
        "verse_152": "وَلَدَ ٱللَّهُ وَإِنَّهُمْ لَكَٰذِبُونَ",
        "verse_153": "أَصْطَفَى ٱلْبَنَاتِ عَلَى ٱلْبَنِينَ",
        "verse_154": "مَا لَكُمْ كَيْفَ تَحْكُمُونَ",
        "verse_155": "أَفَلَا تَذَكَّرُونَ",
        "verse_156": "أَمْ لَكُمْ سُلْطَٰنٌۭ مُّبِينٌۭ",
        "verse_157": "فَأْتُوا۟ بِكِتَٰبِكُمْ إِن كُنتُمْ صَٰدِقِينَ",
        "verse_158": "وَجَعَلُوا۟ بَيْنَهُۥ وَبَيْنَ ٱلْجِنَّةِ نَسَبًۭا ۚ وَلَقَدْ عَلِمَتِ ٱلْجِنَّةُ إِنَّهُمْ لَمُحْضَرُونَ",
        "verse_159": "سُبْحَٰنَ ٱللَّهِ عَمَّا يَصِفُونَ",
        "verse_160": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
        "verse_161": "فَإِنَّكُمْ وَمَا تَعْبُدُونَ",
        "verse_162": "مَآ أَنتُمْ عَلَيْهِ بِفَٰتِنِينَ",
        "verse_163": "إِلَّا مَنْ هُوَ صَالِ ٱلْجَحِيمِ",
        "verse_164": "وَمَا مِنَّآ إِلَّا لَهُۥ مَقَامٌۭ مَّعْلُومٌۭ",
        "verse_165": "وَإِنَّا لَنَحْنُ ٱلصَّآفُّونَ",
        "verse_166": "وَإِنَّا لَنَحْنُ ٱلْمُسَبِّحُونَ",
        "verse_167": "وَإِن كَانُوا۟ لَيَقُولُونَ",
        "verse_168": "لَوْ أَنَّ عِندَنَا ذِكْرًۭا مِّنَ ٱلْأَوَّلِينَ",
        "verse_169": "لَكُنَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
        "verse_170": "فَكَفَرُوا۟ بِهِۦ ۖ فَسَوْفَ يَعْلَمُونَ",
        "verse_171": "وَلَقَدْ سَبَقَتْ كَلِمَتُنَا لِعِبَادِنَا ٱلْمُرْسَلِينَ",
        "verse_172": "إِنَّهُمْ لَهُمُ ٱلْمَنصُورُونَ",
        "verse_173": "وَإِنَّ جُندَنَا لَهُمُ ٱلْغَٰلِبُونَ",
        "verse_174": "فَتَوَلَّ عَنْهُمْ حَتَّىٰ حِينٍۢ",
        "verse_175": "وَأَبْصِرْهُمْ فَسَوْفَ يُبْصِرُونَ",
        "verse_176": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَ",
        "verse_177": "فَإِذَا نَزَلَ بِسَاحَتِهِمْ فَسَآءَ صَبَاحُ ٱلْمُنذَرِينَ",
        "verse_178": "وَتَوَلَّ عَنْهُمْ حَتَّىٰ حِينٍۢ",
        "verse_179": "وَأَبْصِرْ فَسَوْفَ يُبْصِرُونَ",
        "verse_180": "سُبْحَٰنَ رَبِّكَ رَبِّ ٱلْعِزَّةِ عَمَّا يَصِفُونَ",
        "verse_181": "وَسَلَٰمٌ عَلَى ٱلْمُرْسَلِينَ",
        "verse_182": "وَٱلْحَمْدُ لِلَّهِ رَبِّ ٱلْعَٰلَمِينَ"
    },
